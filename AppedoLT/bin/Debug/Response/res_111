            function CheckTerms(src,args)
            {
                if(document.getElementById("ctl00_ContentPlaceHolder1_ChkAccept").checked == true)
                    args.IsValid = true;
                else
                    args.IsValid = false;                
            }
//            function CheckCountry(src,args)
//            {                
//                if(document.getElementById("ctl00_ContentPlaceHolder1_selCountry").value == "--Select--")
//                    args.IsValid = false;
//                else
//                    args.IsValid = true;  
//            } 
            
            function CheckCountry(src,args)
            {   
                // Check if the selected country is India then State is enabled otherwise State is disabled
                if(document.getElementById("ctl00_ContentPlaceHolder1_selCountry").value != "India")
			    { 
				    document.getElementById("ctl00_ContentPlaceHolder1_valstate").style.visibility ="hidden";
			        document.getElementById("ctl00_ContentPlaceHolder1_selState").value="[*** If India - Choose one ***]";
				    document.getElementById("ctl00_ContentPlaceHolder1_selState").disabled="disabled";
			    }
			    else
			    {
			        document.getElementById("ctl00_ContentPlaceHolder1_valstate").style.visibility ="visible";
				    document.getElementById("ctl00_ContentPlaceHolder1_selState").disabled="";
			    }
			
                if(document.getElementById("ctl00_ContentPlaceHolder1_selCountry").value == "--Select--")
                    args.IsValid = false;
                else
                    args.IsValid = true;  
            } 
            
            function CheckState(src,args)
            {                
                var chkCountry = "ctl00_ContentPlaceHolder1_selCountry"
                var myCountry = document.getElementById(chkCountry).value;
                if(myCountry == "India")
                 {
                    if(document.getElementById("ctl00_ContentPlaceHolder1_selState").value == "[*** If India - Choose one ***]")
                        args.IsValid = false;
                     else
                        args.IsValid = true;
                 }  
            } 
            function CheckGender(src,args)
            {
                if(document.getElementById("ctl00_ContentPlaceHolder1_ddlGender").selectedIndex == 0 || document.getElementById("ctl00_ContentPlaceHolder1_ddlGender").value =="--Select--") 
                    args.IsValid = false;
                else
                    args.IsValid = true;  
            }
            function ValidateFileUpload(Source, args)
            {
            
             // var fuData = document.getElementById('ctl00_ContentPlaceHolder1_txtUpload'); 
             
              //var FileUploadPath = fuData.value;
              
              var FileUploadPath = args.Value;
             
              if(FileUploadPath =='') 
              {
                // There is no file selected 
                args.IsValid = false;
              }
              else
              {
                var Extension = FileUploadPath.substring(FileUploadPath.lastIndexOf('.') + 1).toLowerCase();
             
                if (Extension == "gif" || Extension == "jpg" || Extension == "jpeg" ||  Extension =="bmp")
                {
                  args.IsValid = true; // Valid file type
                }
                else
                {
                  args.IsValid = false; // Not valid file type
                }
               }
            }
            function CheckPhotoCard(src,args)
            {
                if(document.getElementById("ctl00_ContentPlaceHolder1_selPhoto").value == "Choose One")
                    args.IsValid = false;
                else
                    args.IsValid = true;
            }  
            
           // Function to validate passport - should allow only letters and numbers
         function CheckPassport(src,args)
            {
                var passRegx = /^(\w[0-9a-zA-Z]*)?$/i;                
                var passportNo = document.getElementById("ctl00_ContentPlaceHolder1_txtIDCardPP").value;
                var result = passRegx.test(passportNo);                
               
                if(result == true && passportNo.length >= 7)
                {
                    args.IsValid = true;
                }
                else
                {
                    args.IsValid = false;
                }                
            }
            
            // Function to validate driving licence - should allow only letters and numbers and space,hyphen and slash as special characters
          function CheckDrivingLicence(src,args)
            {
                var passRegx = /^(\w[0-9 a-zA-Z-/]*)?$/i;        
                var numberic = "0123456789"; 
                var alphabets = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";        
                var drivingLicenceNo = document.getElementById("ctl00_ContentPlaceHolder1_txtIDCardDL").value;
                var result = passRegx.test(drivingLicenceNo);                
                noCount = 0; 
                letterCount = 0;
                
                for(i=0;i<drivingLicenceNo.length;i++)
                {  
                    for(j=0;j<numberic.length;j++)
                    {                             
                        if(drivingLicenceNo.charAt(i) == numberic.charAt(j))
                        {
                            noCount = noCount + 1; 
                        }
                    }                    
                }
                for(i=0;i<drivingLicenceNo.length;i++)
                {  
                    for(j=0;j<alphabets.length;j++)
                    {                             
                        if(drivingLicenceNo.charAt(i) == alphabets.charAt(j))
                        {
                            letterCount = letterCount + 1;
                        }
                    }                    
                }                
                
                if(result == true && drivingLicenceNo.length >= 7)
                {
                    args.IsValid = true;
                }
                else
                {
                    args.IsValid = false;
                }                
            }
            
            // Function to validate voters id - should allow only letters and numbers, some special characters allowed
            // Allowed characters are blankspace,/,- and _
            // Updated by Hariselvan.N on Feb-01-2012 - Special allowed for DrivingLicence
            function CheckVotersID(src,args)
            {
                //var votersIDRegx = /^(\w[0-9a-zA-Z]*)?$/i; 
                var votersIDRegx = /^(\w[0-9 a-zA-Z/-]*)?$/i;                
                var votersIDNo = document.getElementById("ctl00_ContentPlaceHolder1_txtIDCardVID").value;
                var result = votersIDRegx.test(votersIDNo);                
               
                if(result == true && votersIDNo.length >= 7)
                {
                    args.IsValid = true;
                }
                else
                {
                    args.IsValid = false;
                }                
            }    
            
            // ------ CR000356
            function CheckSchoolID(src,args)
            {
                //var schoolIDRegx = /^(\w[0-9a-zA-Z/-_]*)?$/i;  
                var schoolIDRegx = "~!@#$%^&*()+=|]}[{';:,<.>?` \"";       
                var validSchoolIDRegx = "-_/";                    
                var schoolIDNo = document.getElementById("ctl00_ContentPlaceHolder1_txtIDCardSchool").value;
                //var result = schoolIDRegx.test(schoolIDNo);  
                var kount = 0;
                var kount1 = 0;
                var specialCheck = false;
                var firstSpecialCheck = false;
                
                    for(k=0;k<validSchoolIDRegx.length; k++)
                    {
                        if(schoolIDNo.charAt(0) == validSchoolIDRegx.charAt(k))
                        {                           
                            firstSpecialCheck = true;
                            break;
                        }
                    } 
               
                for(i=0;i<schoolIDNo.length; i++)
                {
                    for(j=0;j<schoolIDRegx.length; j++)
                    {
                        if(schoolIDNo.charAt(i)== schoolIDRegx.charAt(j))
                        {
                            kount = 1;
                            break;
                        }
                    } 
                    if(kount == 1)
                    {
                        specialCheck = true;
                        break;
                    }  
                }              
              
                if(specialCheck == false && schoolIDNo.length >= 6 && firstSpecialCheck == false)
                {
                    args.IsValid = true;
                }
                else
                {
                    args.IsValid = false;
                }  
                          
            }
            // ------------ CR000356
            
            function PhoneAndMobileValidation(src,args)
            {
                var phoneNoRegx = /^([0-9]*)?$/i;                
                var phoneNo = args.Value;
                var result = phoneNoRegx.test(phoneNo);                
                var noCount = 0;
                
                if(phoneNo.charAt(0) == 0)
                {
                   noCount = noCount + 1; 
                }
                if(phoneNo.charAt(1) == 0)
                {
                   noCount = noCount + 1; 
                }
                if(phoneNo.charAt(2) == 0)
                {
                   noCount = noCount + 1; 
                }
                
                if(result == true && noCount < 3 && phoneNo.length >= 10)
                {
                    args.IsValid = true;
                }
                else if(result == true && noCount >= 3)
                {
                    args.IsValid = false;
                } 
                else if(result == false && noCount >= 3)
                {
                    args.IsValid = false;
                } 
                else if(result == false && noCount < 3)
                {
                    args.IsValid = false;
                } 
                else 
                {
                    args.IsValid = false;
                }                  
            }
   
    </script>

